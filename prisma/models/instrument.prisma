model Instrument {
    // ID
    id String @id @default(uuid())

    // Required Attributes
    name String @unique

    // Optional Attributes
    description String?

    // Metadata
    created_at DateTime @default(now())
    updated_at DateTime @default(now()) @updatedAt

    // Relations
    PracticeParts  PracticePart[]
    UserInstrument UserInstrument[]
}
